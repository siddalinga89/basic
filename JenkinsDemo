pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building '
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Delevery') {
            steps {
                echo 'Deleverying'
            }
        }
        stage('Deploye') {
            steps {
                echo 'Deploying'
            }
        }
    }
}
